<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="590.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lunarapps.hakuna.mainOps.SignupController">
   <children>
      <TextField fx:id="firstname" layoutX="207.0" layoutY="118.0" onMouseExited="#checkNameRegex">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="125.0" layoutY="122.0" text="First Name :" />
      <Label layoutX="125.0" layoutY="171.0" text="Last Name :" />
      <TextField fx:id="lastname" layoutX="207.0" layoutY="167.0" onMouseExited="#checkLastRegex">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="125.0" layoutY="308.0" text="Email :" />
      <Label layoutX="125.0" layoutY="355.0" text="Username :" />
      <Label layoutX="125.0" layoutY="400.0" text="Password :" />
      <Label layoutX="126.0" layoutY="448.0" text="Confirm :" />
      <Label layoutX="125.0" layoutY="220.0" text="Sex :" />
      <TextField fx:id="email" layoutX="207.0" layoutY="304.0" onMouseExited="#checkEmailRegex" prefHeight="26.0" prefWidth="198.0" />
      <TextField fx:id="chosenUsername" layoutX="207.0" layoutY="346.0" prefHeight="26.0" prefWidth="198.0" />
      <TextField fx:id="chosenPass" layoutX="207.0" layoutY="396.0" onMouseExited="#checkPassRegex" prefHeight="26.0" prefWidth="198.0" />
      <TextField fx:id="confirmedPass" layoutX="207.0" layoutY="443.0" onMouseExited="#checkConfirm" prefHeight="26.0" prefWidth="198.0" />
      <Button fx:id="createAccountBtn" layoutX="165.0" layoutY="496.0" mnemonicParsing="false" onMouseClicked="#perfCreateAcct" prefHeight="32.0" prefWidth="198.0" text="Create Account" />
      <CheckBox fx:id="male" layoutX="211.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#selectOne" text="Male" />
      <CheckBox fx:id="female" layoutX="297.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#selectOne" text="Female" />
      <TextField fx:id="messageField" editable="false" layoutX="168.0" layoutY="541.0" opacity="0.46" prefHeight="25.0" prefWidth="198.0" />

   </children>
</AnchorPane>
